    "@remix-run/react": "^2.5.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
